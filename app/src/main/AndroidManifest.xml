<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.yidian.adsdkdemo">

    <application
        android:name="com.yidian.geek.addemo.YdApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:networkSecurityConfig="@xml/ad_network_security_config"
        tools:replace="android:networkSecurityConfig">

        <activity android:name="com.yidian.geek.addemo.page.SplashActivity"
            android:theme="@style/WindowFullscreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.yidian.geek.addemo.page.NaviHomeActivity" />
    </application>

</manifest>